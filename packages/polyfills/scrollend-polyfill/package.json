{
    "name": "@af-utils/scrollend-polyfill",
    "private": false,
    "version": "0.0.10",
    "description": "Polyfill for scrollend event",
    "repository": {
        "type": "git",
        "url": "https://github.com/nowaalex/af-utils.git",
        "directory": "packages/polyfills/scrollend-polyfill"
    },
    "homepage": "https://af-utils.com/scrollend-polyfill",
    "bugs": "https://github.com/nowaalex/af-utils/issues",
    "author": {
        "name": "Alex Fomin",
        "email": "nowaalex@gmail.com",
        "url": "https://github.com/nowaalex/"
    },
    "license": "MIT",
    "scripts": {
        "dev": "pnpm export-bundle-size && pnpm build:types && pnpm esbuild:dev",
        "build": "pnpm build:types && pnpm esbuild:build && pnpm export-bundle-size",
        "build:types": "tsc && tsc-alias",
        "esbuild:build": "node build.mjs prod",
        "esbuild:dev": "node build.mjs",
        "export-bundle-size": "export-bundle-size ./lib/index.js"
    },
    "files": [
        "lib/*"
    ],
    "type": "module",
    "types": "lib/index.d.ts",
    "main": "lib/index.js",
    "exports": {
        "./README.md": "./README.md",
        "./lib/bundlesize.index.js": "./lib/bundlesize.index.js",
        ".": {
            "types": "./lib/index.d.ts",
            "node": "./lib/index.js",
            "default": "./lib/index.js"
        }
    },
    "keywords": [
        "scrollend",
        "polyfill"
    ],
    "devDependencies": {
        "@af-utils/export-bundle-size": "workspace:*",
        "esbuild": "^0.20.2",
        "typescript": "^5.4.5"
    },
    "publishConfig": {
        "access": "public"
    }
}
