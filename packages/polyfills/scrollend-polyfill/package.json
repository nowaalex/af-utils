{
    "name": "@af-utils/scrollend-polyfill",
    "private": false,
    "version": "0.0.10",
    "description": "Polyfill for scrollend event",
    "repository": {
        "type": "git",
        "url": "https://github.com/nowaalex/af-utils.git",
        "directory": "packages/polyfills/scrollend-polyfill"
    },
    "homepage": "https://af-utils.com/scrollend-polyfill",
    "bugs": "https://github.com/nowaalex/af-utils/issues",
    "author": "Alex Fomin <nowaalex@gmail.com> (https://github.com/nowaalex/)",
    "license": "MIT",
    "scripts": {
        "dev": "rollup -w -c rollup.config.ts --configPlugin typescript && pnpm export-bundle-size",
        "build": "rollup -c rollup.config.ts --configPlugin typescript && pnpm export-bundle-size",
        "export-bundle-size": "export-bundle-size ./lib/index.js"
    },
    "files": [
        "lib/*"
    ],
    "type": "module",
    "types": "lib/index.d.ts",
    "browser": "lib/index.js",
    "main": "lib/index.js",
    "keywords": [
        "scrollend",
        "polyfill"
    ],
    "devDependencies": {
        "@af-utils/export-bundle-size": "workspace:*",
        "@rollup/plugin-terser": "^0.4.4",
        "@rollup/plugin-typescript": "^11.1.6",
        "rollup": "^4.14.1"
    },
    "publishConfig": {
        "access": "public"
    }
}
