{
    "compilerOptions": {
        "lib": ["DOM", "DOM.Iterable", "ES2023"],
        "moduleResolution": "Bundler",
        "module": "ESNext",
        "target": "ESNext",
        "allowSyntheticDefaultImports": true,
        "strict": true,
        "alwaysStrict": true,
        "resolveJsonModule": true,
        "skipLibCheck": true
    }
}
